output
typedef struct _OUTT {
  UINT8   Saw_f:4;
  UINT8   Saw_g:4;
  UINT32  O_f;
  UINT16  O_g;
} OUTT;

casetype _OPTION(UINT8 Kind, mutable OUTT *out) {
  switch(Kind) {
    case 0:
      UINT32    f
      {:on-success
        out->Saw_f = 1;
        out->O_f = f;
        return true;};
    default:
      UINT16    g
      {:on-success
        out->Saw_g = 1;
        out->O_g = g;  
        return true;};
  }
} OPTION;

entrypoint
typedef struct _T(mutable OUTT *Out) {
  UINT8               Kind;
  OPTION(Kind, Out)   Payload;
} T;
