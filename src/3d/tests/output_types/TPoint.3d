typedef struct _OPOINT {
    UINT8   x;
    UINT32  y;
} OPOINT;

output
typedef struct _OTPOINT {
    OPOINT    op;
    UINT64    oz;
} OTPOINT;

typedef struct _POINT (mutable OPOINT* out) {
    UINT8  x
    {:on-success
        out->x = x;
        return true;
    };
    UINT32 y
    {:on-success
        out->y = y;
        return true;
    }
} POINT;

entrypoint
typedef struct _TPOINT (mutable OTPOINT* out) {
    UINT64            z
    {:on-success
        out->oz = z;
        return true;
    };
    POINT(&(out->op)) p;
} TPOINT;

entrypoint
typedef struct _FLATTPOINT (mutable OTPOINT* out) {
    UINT8 x
    {:on-success
        (out->op).x = x;
        return true;
    };
    UINT32 y
    {:on-success
        (out->op).y = y;
        return true;
    };
    UINT64 z
    {:on-success
        out->oz = z;
        return true;
    };
} FLATTPOINT;
