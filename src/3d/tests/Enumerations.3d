//Simple enum
UINT8 enum Enum8 {
   Enum8_1 = 0,
   Enum8_2,
   Enum8_3
}

//Enum with lots of intermediate values and repetition
UINT32 enum Enum32 {
    Enum32_0 = 0,
    Enum32_1 = 1,
    Enum32_2 = 2,
    Enum32_Next = 100,
    Enum32_A = Enum32_Next,
    Enum32_B,
    Enum32_C,
    Enum32_AlphaMax = Enum32_C,
    Enum32_More_0,
    Enum32_More_1
}

//Enum of pre-defined constants
#define DEF_A 81
#define DEF_B 43
#define DEF_C 141
UINT8 enum Enum_ABC {
    DEF_A,
    DEF_B,
    DEF_C
}

//Enum of pre-defined constants in a given type
#define DEF_A32 81ul
#define DEF_B32 43ul
#define DEF_C32 141ul
UINT32 enum Enum_ABC32 {
    DEF_A32,
    DEF_B32,
    DEF_C32
}

//need an entry point or 3d will reject it
entrypoint
typedef struct _Dummy {
  Enum8 e_8;
  Enum32 e_32;
  Enum_ABC e_abc;
  Enum_ABC32 e_abc32;
} Dummy;
